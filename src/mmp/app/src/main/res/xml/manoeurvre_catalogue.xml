         <component pitch="MIN" roll="ZERO" yaw="ZERO" length="0.1" />
         <component pitch="ZERO" roll="ZERO" yaw="ZERO" length="1" />
      </variant>
      <variant type="i" name="inverted z-figure">
         <component pitch="ZERO" roll="ZERO" yaw="ZERO" length="1" />
         <component pitch="MIN" roll="ZERO" yaw="ZERO" length="0.1" />
         <component pitch="MIN" roll="ZERO" yaw="ZERO" length="0.1" />
