         <component pitch="ZERO" roll="ZERO" yaw="ZERO" length="1" />
         <component pitch="MAX" roll="ZERO" yaw="ZERO" length="0.1" />
         <component pitch="MAX" roll="ZERO" yaw="ZERO" length="0.1" />
         <component pitch="ZERO" roll="ZERO" yaw="ZERO" length="1" />
      </variant>
   </manoeuvre>

   <manoeuvre olan="z">
