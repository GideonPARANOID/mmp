     <text:p text:style-name="P86"><text:tab/><text:a xlink:type="simple" xlink:href="#__RefHeading__2114_1315851627" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"> 16 Code Samples<text:tab/>39</text:a></text:p>
     <text:h text:style-name="P84" text:outline-level="1"><text:a xlink:type="simple" xlink:href="#__RefHeading__2116_1315851627" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:s/>17 Annotated Bibliography<text:tab/>40</text:a></text:h>
